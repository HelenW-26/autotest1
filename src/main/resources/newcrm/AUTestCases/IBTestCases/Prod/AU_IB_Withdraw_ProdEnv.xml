<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AU Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  <test name="AU IB Portal">
    <parameter name="Brand" value="VFX"></parameter>
  	<parameter name="TestEnv" value="prod"></parameter>
  	<parameter name="Regulator" value="vfsc2"></parameter>
  	<parameter name="TraderURL" value="https://secure.vantagefx.com"></parameter>
   	<parameter name="TraderName" value="ibtestcaj@test.com"></parameter>
    <parameter name="TraderPass" value="123Qwe"></parameter>  
    <classes>
   		 <class name="newcrm.testcases.ibtestcases.IBRebateWithdrawTestcases">
   		 	<methods>
   		 		<include name="testSkrillWithdraw"></include>
   		 		<include name="testNetellerWithdraw"></include>
   		 		<include name="testSticPayWithdraw"></include>
   		 		<include name="testCryptoBitcoinWithdraw"></include>
   		 		<include name="testCryptoUSDTWithdraw"></include> 
   		 		<include name="testBitwalletWithdraw"></include>
   		 		<include name="testFasaPayWithdraw"></include>
   		 		<include name="testUnionPayWithdraw"></include>
   		 		<include name="testInternationalBankNewAccountWitdraw"></include>
   		 		<include name="testInternationalBankSavedAccountWitdraw"></include>
   		 		<!-- <include name="testAustraliaBankNewAccountWitdraw"></include>
   		 		<include name="testAustraliaBankSavedAccountWitdraw"></include> -->
   		 	</methods>
   		 </class>   		      		
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
