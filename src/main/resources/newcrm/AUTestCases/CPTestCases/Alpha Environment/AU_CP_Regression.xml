<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AU Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  <test name="AU Regression Test">
    <parameter name="Brand" value="VFX"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
  	<parameter name="Regulator" value="vfsc"></parameter>
    <parameter name="AdminURL" value="https://admin-capricorn-ex.crm-alpha.com"></parameter>
  	<parameter name="AdminName" value="Yanni"></parameter>
  	<parameter name="AdminPass" value="12345678Qi"></parameter>
  	<parameter name="TraderURL" value="https://secure-capricorn-ex.crm-alpha.com"></parameter>
   	<parameter name="TraderName" value="VFXAutomationexclusive@mailinator.com"></parameter>
    <parameter name="TraderPass" value="123Qwe"></parameter>
    <parameter name="OpenAPIURL" value="https://openapi-capricorn-ex.crm-alpha.com"></parameter>
    
    <parameter name="headless" value="true"></parameter>  
    
    <groups>
    	<run>
    		<include name="AuRegression"></include>
    	</run>
    </groups>
    <classes>
    	<class name="newcrm.testcases.cptestcases.RegisterTestcases"/>
    	<class name="newcrm.testcases.cptestcases.DepositTestCases"/>
    	<class name="newcrm.testcases.cptestcases.WithdrawTestCases"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
