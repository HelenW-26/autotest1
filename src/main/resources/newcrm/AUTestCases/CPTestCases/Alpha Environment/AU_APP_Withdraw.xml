<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="VFX Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  	<parameter name="Brand" value="VFX"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
  	<parameter name="Regulator" value="vfsc"></parameter>
  	<parameter name="APPURL" value="https://app-cps01.crm-alpha.com"></parameter>
  		
  	<test name="VFX Test APP CPS Malaysia Bank Transfer Withdrawal">
   	<parameter name="UserId" value="10009191"></parameter>
   	<parameter name="AccountNo" value="804308791"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsMYBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>
  	<test name="VFX Test APP CPS Vietnam Bank Transfer Withdrawal - batch 3">
  	<parameter name="Regulator" value="vfsc2"></parameter>
   	<parameter name="UserId" value="10020297"></parameter>
   	<parameter name="AccountNo" value="804376341"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsVNBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>  
  	<test name="VFX Test APP CPS India Bank Transfer Withdrawal - batch 3">
  	<parameter name="Regulator" value="vfsc2"></parameter>
   	<parameter name="UserId" value="10020591"></parameter>
   	<parameter name="AccountNo" value="804411792"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsINBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>  
  
  <!-- Test -->
</suite> <!-- Suite -->
