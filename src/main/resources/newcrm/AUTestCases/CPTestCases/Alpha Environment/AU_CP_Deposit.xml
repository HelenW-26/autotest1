<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AU Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
	<parameter name="Brand" value="VFX"></parameter>
	<parameter name="TestEnv" value="alpha"></parameter>
	<parameter name="Regulator" value="vfsc2"></parameter>
	<parameter name="TraderURL" value="https://secure-core-au.crm-alpha.com"></parameter>
	<parameter name="TraderPass" value="123Qwe"></parameter>
	<parameter name="headless" value="true"></parameter>

  		<test name="AU Test Non-BankTransfer Deposit">
            <parameter name="TraderName" value="testcrmautomation_donttouchsouthnonbt@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                    <methods>
                         <include name="testBridgePay"></include>
                         <include name="testCPSCryptoDeposit"></include>
                         <include name="testInterBankTransPay"></include>
                        <include name="testNetellerPayWithCallback"></include>
                        <include name="testSkrillPay"></include>
                          <!-- <include name="testFasaPay"></include> Valarie: Turned off in CPS Group Chat --> 
                    </methods>
                </class>
            </classes>
        </test> 
      <test name="AU Test South Korea CP Deposit">
          <parameter name="TraderName" value="testcrmautomation_donttouchsouthkorea@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                        <methods>
                            <include name="testSKBankTransfer"></include>
                            <include name="testSKEbuyPay"></include>
                    </methods>
                </class>
        </classes>
      </test> 
         <test name="AU Test Malaysia CP Deposit">
            <parameter name="TraderName" value="testcrmautomation_donttouchsouthmalaysia@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                    <methods>
                        <include name="testMYBankTransfer"></include>
                        <include name="testMYEwallet"></include>
                    </methods>
                </class>
            </classes>
        </test> 
         <test name="AU Test Indonesia CP Deposit">
            <parameter name="TraderName" value="testcrmautomation_donttouchsouthindonesia@test.com"></parameter>
            <classes>
                    <class name="newcrm.testcases.cptestcases.DepositTestCases">
                        <methods>
                            <include name="testIndonesiaBankTransferEeziepay"></include>
                            <include name="testIndonesiaBankTransferTransact365"></include>
                            <include name="testIndonesiaBankTransferZotapay"></include>
                    </methods>
                </class>
            </classes>
         </test>
        <test name="AU Test Vietnam CP Deposit">
            <parameter name="TraderName" value="testcrmautomation_donttouchsouthvietnam@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                    <methods>
                        <include name="testVietnamBankTransferEeziepay"></include>
                        <include name="testVietnamBankTransferZotapay"></include>
                    </methods>
                </class>
            </classes>
        </test>   
</suite> <!-- Suite -->
