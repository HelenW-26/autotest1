		<parameter name="AdminURL" value="https://admin-earth-ex.crm-alpha.com/"></parameter>
		<parameter name="AdminName" value="lynn"></parameter>
		<parameter name="AdminPass" value="123Qwe"></parameter>
		<parameter name="TraderURL" value="https://secure-staging-au-ex.crm-alpha.com"></parameter>
		<parameter name="TraderName" value="automationcreditcardexclusive@mailinator.com"></parameter>
		<parameter name="TraderPass" value="123Qwe"></parameter>
		<classes>
			<class name="newcrm.testcases.cptestcases.DepositTestCases">
			 	<methods>
					<include name="testBridgePay"></include>
				</methods> 
			</class>
				 <class name="newcrm.testcases.cptestcases.WithdrawTestCases">
   		 	<methods>
