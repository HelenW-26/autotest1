<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="PU Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  	<parameter name="Brand" value="PUG"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
  	<parameter name="Regulator" value="svg"></parameter>
  	<parameter name="APPURL" value="https://app-cps03.crm-alpha.com"></parameter>
  		
  	<test name="PU Test APP CPS Malaysia Bank Transfer Withdrawal">
   	<parameter name="UserId" value="10003538"></parameter>
   	<parameter name="AccountNo" value="90210436"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsMYBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>
  	<test name="PU Test APP CPS Philippines Bank Transfer Withdrawal - batch 3">
   	<parameter name="UserId" value="10010845"></parameter>
   	<parameter name="AccountNo" value="90210499"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsPHBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>  

  
  <!-- Test -->
</suite> <!-- Suite -->
