<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MO Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
	<parameter name="Brand" value="MO"></parameter>
	<parameter name="TestEnv" value="alpha"></parameter>
	<parameter name="Regulator" value="vfsc"></parameter>
	<parameter name="TraderURL" value="https://secure-core-mo.crm-alpha.com"></parameter>
	<parameter name="TraderPass" value="123Qwe"></parameter>
	<parameter name="headless" value="true"></parameter>

   		<test name="MO Test Non-BankTransfer Deposit">
            <parameter name="TraderName" value="testcrm_donttouchnonbt@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                    <methods>
                          <include name="testCreditCardBridgePay"></include>
                          <include name="testCPSCryptoDeposit"></include>
                          <include name="testInterBankTransPay"></include>
                    </methods>
                </class>
            </classes>
        </test> 
       <test name="MO Test South Korea CP Deposit">
          <parameter name="TraderName" value="testcrm_donttouchnsouthkorea@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                        <methods>
                            <include name="testSKBankTransfer"></include>
                    </methods>
                </class>
        </classes>
      </test> 
         <test name="MO Test Malaysia CP Deposit">
            <parameter name="TraderName" value="testcrm_donttouchnmalaysia@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                    <methods>
                        <include name="testMYBankTransfer"></include>
                    </methods>
                </class>
            </classes>
        </test>
          <test name="MO Test Indonesia CP Deposit">
            <parameter name="TraderName" value="testcrm_donttouchnindonesia@test.com"></parameter>
            <classes>
                    <class name="newcrm.testcases.cptestcases.DepositTestCases">
                        <methods>
                            <include name="testIndonesiaBankTransferEeziepay"></include>
                            <include name="testIndonesiaBankTransferTransact365"></include>
                    </methods>
                </class>
            </classes>
         </test>
         <test name="MO Test Vietnam CP Deposit">
            <parameter name="TraderName" value="testcrm_donttouchnvietnam@test.com"></parameter>
            <classes>
                <class name="newcrm.testcases.cptestcases.DepositTestCases">
                    <methods>
                        <include name="testVietnamBankTransferEeziepay"></include>
                    </methods>
                </class>
            </classes>
        </test> 
         <test name="MO Test Thailand CP Deposit">
            <parameter name="TraderName" value="testcrm_donttouchnthailand@test.com"></parameter>
            <classes>
                    <class name="newcrm.testcases.cptestcases.DepositTestCases">
                        <methods>
                            <include name="testThaiBankEeziePay"></include>
                    </methods>
                </class>
            </classes>
         </test> 
           <!-- Test -->
</suite> <!-- Suite -->
