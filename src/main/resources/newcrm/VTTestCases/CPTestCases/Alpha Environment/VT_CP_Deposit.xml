<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="VT Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
	<parameter name="Brand" value="VT"></parameter>
	<parameter name="TestEnv" value="alpha"></parameter>
	<parameter name="Regulator" value="svg"></parameter>
	<parameter name="TraderURL" value="https://secure-core-vt.crm-alpha.com"></parameter>
	<parameter name="TraderPass" value="123Qwe"></parameter>
	<parameter name="headless" value="true"></parameter>
	<parameter name="OpenApi" value="https://openapi-core-vt.crm-alpha.com"></parameter>
		<test name="VT Test Credit Card Deposit">
            <parameter name="TraderName" value="autotestnidgirig@testcrmautomation.com"></parameter>
