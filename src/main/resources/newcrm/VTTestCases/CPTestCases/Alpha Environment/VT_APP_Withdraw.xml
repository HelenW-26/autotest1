<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="VT Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  	<parameter name="Brand" value="VT"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
  	<parameter name="Regulator" value="svg"></parameter>
  	<parameter name="APPURL" value="https://app-cps02.crm-alpha.com"></parameter>
  		
    <test name="VT Test APP CPS Indonesia Bank Transfer Withdrawal">
   	<parameter name="UserId" value="10014462"></parameter>
   	<parameter name="AccountNo" value="810281611"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsIDBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>
  	<test name="VT Test APP CPS Malaysia Bank Transfer Withdrawal">
   	<parameter name="UserId" value="10014088"></parameter>
   	<parameter name="AccountNo" value="810022841"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsMYBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>
  	<test name="VT Test APP CPS Thailand Bank Transfer Withdrawal - batch 3">
   	<parameter name="UserId" value="10017163"></parameter>
   	<parameter name="AccountNo" value="810023050"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsTHBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>
  	<test name="VT Test APP CPS Vietnam Bank Transfer Withdrawal - batch 3">
   	<parameter name="UserId" value="10009874"></parameter>
   	<parameter name="AccountNo" value="804394749"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsVNBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test>  
  
  
  <!-- Test -->
</suite> <!-- Suite -->
