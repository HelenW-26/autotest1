<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Add User to DB whitelist" configfailurepolicy="continue">

 <listeners>
     <listener class-name="utils.Listeners.CustomListener"/>
  </listeners>

	<parameter name="Brand" value="${brand}"></parameter>
	<parameter name="email" value="${email}"></parameter>

	<test name="Add user's accounts to tb_account_change_whitelist">
		<classes>
			<class name="newcrm.testcases.others.OtherFunctionalTests">
				<methods>
					<include name="whitelistAccountsByEmail"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite><!-- Suite -->