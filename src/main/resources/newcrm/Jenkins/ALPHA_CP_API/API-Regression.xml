<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- PROD REGRESSION /  QA_AUTOMATION_81_VT_PROD_REGISTER_REGRESSION-->

<suite name="API Deposit" configfailurepolicy="continue">

    <listeners>
        <listener class-name="utils.Listeners.CustomListener"/>
        <!-- <listener class-name="utils.Listeners.RetryListener"/> -->
    </listeners>

    <!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod".
    When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->

    <parameter name="TestEnv" value="ALPHA"></parameter>
    <parameter name="Debug" value="true"></parameter>
    
	<!-- AU -->
    <test name="AU_VFSC_Japan_deposit">
        <parameter name="Brand" value="VFX"></parameter>
        <parameter name="Regulator" value="VFSC2"></parameter>
        <parameter name="TraderURL" value="https://secure-core-au.crm-alpha.com/"></parameter>
        <parameter name="TraderName" value="bbce@mailinator.com"></parameter>
        <parameter name="TraderPass" value="123Qwe"></parameter>
        <classes>
            <class name="newcrm.testcases.crmapi.CPAPIDepositTestcases">
                <methods>
                    <include name="apiNetellerDeposit"></include>
                    <include name="apiBridgepayDeposit"></include>
                    <include name="apiSticpayDeposit"></include>
					<include name="apiSkrillpayDeposit"></include>
                </methods>
            </class>
            <class name="newcrm.testcases.crmapi.CPAPIWithdrawTestcases">
                <methods>
                    <include name="apiwithdraw"></include>
                    <!--<include name="apiWithdrawCancelAll"></include>
                    <include name="getdeductcredit"></include>-->
                </methods>
            </class>
        </classes>
    </test>
    <test name="AU_VFSC_Germany_deposit">
        <parameter name="Brand" value="VFX"></parameter>
        <parameter name="Regulator" value="VFSC2"></parameter>
        <parameter name="TraderURL" value="https://secure-core-au.crm-alpha.com/"></parameter>
        <parameter name="TraderName" value="testcrm@tradingbonus.test.com"></parameter>
        <parameter name="TraderPass" value="123Qwe"></parameter>
        <classes>
            <class name="newcrm.testcases.crmapi.CPAPIDepositTestcases">
                <methods>
					<include name="apiAstropayDeposit"></include>
                </methods>
            </class>
        </classes>
    </test>
    <!-- VT -->
    <test name="VT_SVG_deposit">
        <parameter name="Brand" value="VT"></parameter>
        <parameter name="Regulator" value="SVG"></parameter>
        <parameter name="TraderURL" value="https://secure-core-vt.crm-alpha.com/"></parameter>
        <parameter name="TraderName" value="umpzyotx068@appapiautotest.com"></parameter>
        <parameter name="TraderPass" value="123Qwe"></parameter>
        <classes>
            <class name="newcrm.testcases.crmapi.CPAPIDepositTestcases">
                <methods>
                    <include name="apiNetellerDeposit"></include>
                    <include name="apiBridgepayDeposit"></include>
					<include name="apiSkrillpayDeposit"></include>
                </methods>
            </class>
            <class name="newcrm.testcases.crmapi.CPAPIWithdrawTestcases">
                <methods>
                    <include name="apiwithdraw"></include>
                    <exclude name="getdeductcredit"></exclude>
                </methods>
            </class>
        </classes>
    </test>
    
	<!-- PUG -->
	<test name="PUG_SVG_deposit">
		<parameter name="Brand" value="PUG"></parameter>
        <parameter name="Regulator" value="SVG"></parameter>
        <parameter name="TraderURL" value="https://secure-monet.crm-alpha.com/"></parameter>
        <parameter name="TraderName" value="testcrmwojde@test.com"></parameter>
        <parameter name="TraderPass" value="123Qwe"></parameter>
		<classes>
			<class name="newcrm.testcases.crmapi.CPAPIDepositTestcases">
				<methods>
					<include name="apiNetellerDeposit"></include>
					<include name="apiSticpayDeposit"></include>
					<include name="apiSkrillpayDeposit"></include>
				</methods>
			</class>
		</classes>
	</test>
	<!-- MO -->
    <test name="MO_VFSC_Malaysia_deposit">
        <parameter name="Brand" value="MO"></parameter>
        <parameter name="Regulator" value="VFSC"></parameter>
        <parameter name="TraderURL" value="https://secure-cps04.crm-alpha.com/"></parameter>
        <parameter name="TraderName" value="bjiowjfiewfewf@mo.com"></parameter>
        <parameter name="TraderPass" value="123Qwe"></parameter>
        <classes>
            <class name="newcrm.testcases.crmapi.CPAPIDepositTestcases">
                <methods>
                    <include name="apiCPSDeposit"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>
