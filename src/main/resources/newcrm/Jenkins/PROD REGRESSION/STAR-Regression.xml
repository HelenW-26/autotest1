<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- PROD REGRESSION /  QA_AUTOMATION_81_VT_PROD_REGISTER_REGRESSION-->

<suite name="Production Smoke Test" configfailurepolicy="continue">

    <listeners>
        <listener class-name="utils.Listeners.CustomListener"/>
        <!-- <listener class-name="utils.Listeners.RetryListener"/> -->
    </listeners>

    <!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod".
    When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->

    <parameter name="headless" value="true"/>
    <parameter name="Brand" value="STAR"></parameter>
    <parameter name="TestEnv" value="prod"></parameter>
    <parameter name="AdminURL" value="https://admin.startrader.com"></parameter>
    <parameter name="Regulator" value="SVG"></parameter>
    <parameter name="Debug" value="true"></parameter>

    <test name="CP Registration UI Regression">
        <parameter name="IBCode" value="OTkwNDA="></parameter>
        <parameter name="RAFCode" value=""></parameter>
        <parameter name="TraderURL" value="https://myaccount.startrader.com"></parameter>
        <parameter name="TraderName" value="webautomation@test.com"></parameter>
        <parameter name="TraderPass" value="1234Qwer!"></parameter>
        <classes>
            <class name="newcrm.testcases.cptestcases.RegisterTestcases">
                <methods>
                    <include name="testRegistMT4LiveAccountWithoutCheck"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="CP Account, Deposit &amp; Transfer UI Regression">
        <parameter name="IBCode" value="OTkwNDA="></parameter>
        <parameter name="RAFCode" value=""></parameter>
        <parameter name="TraderURL" value="https://myaccount.startrader.com"></parameter>
        <parameter name="TraderName" value="webautomation@test.com"></parameter>
        <parameter name="TraderPass" value="1234Qwer!"></parameter>
        <classes>
            <class name="newcrm.testcases.cptestcases.AccountConfigureTestCases">
                <methods>
                    <include name="testChangeMT4AccountLeverage"></include>
                </methods>
            </class>
            <class name="newcrm.testcases.cptestcases.DepositTestCases">
                <methods>
                    <include name="testSticPayNew"></include>
                    <include name="testDepositCryptoBEPNew"></include>
                </methods>
            </class>
            <class name="newcrm.testcases.cptestcases.AccountManagementTestCases">
                <methods>
                    <include name="testTransfer"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="CP Withdrawal UI Regression">
        <parameter name="TraderURL" value="https://myaccount.startrader.com"></parameter>
        <parameter name="TraderName" value="webautomation@test.com"></parameter>
        <parameter name="TraderPass" value="1234Qwer!"></parameter>
        <parameter name="AdminName" value="Test CRM"></parameter>
        <parameter name="AdminPass" value="BDAVJECnh4hovtZLV33N"></parameter>
        <classes>
            <class name="newcrm.testcases.cptestcases.WithdrawTestCases">
                <methods>
                    <include name="testMalaysiaBankWithdrawNew"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite><!-- Suite -->