<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UM Deposit Test Suite" configfailurepolicy="continue">
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  	<parameter name="Brand" value="UM"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
  	<parameter name="Regulator" value="svg"></parameter>
  	<parameter name="TraderURL" value="${TraderURL}"></parameter>
  	<parameter name="TraderPass" value="123Qwe@@"></parameter>
    <parameter name="headless" value="true"></parameter>

	<!--Batch 1-->
	<test name="UM IBT Deposit">
		<parameter name="TraderName" value="testcrm_donttouch_cpsmalysiaclient@test.com"></parameter>
		<classes>
			<class name="newcrm.testcases.cptestcases.DepositTestCases">
				<methods>
					<include name="testInterBankTransPayNew"></include>
				</methods>
			</class>
		</classes>
	</test>
   <test name="UM Malaysia Deposit">
       <parameter name="TraderName" value="testcrm_donttouch_cpsmalysiaclient@test.com"></parameter>
       <classes>
           <class name="newcrm.testcases.cptestcases.DepositTestCases">
               <methods>
                   <include name="testCryptoTRCNew"></include>
                   <include name="testCryptoERCNew"></include>
                   <!--method disabled, unable upload POF as of June 10 <include name="testCryptoBITNew"></include>-->
                   <include name="testDepositCryptoBEPNew"></include>
                   <include name="testCryptoUSDCERCNew"></include>
                   <include name="cpsMalaysiaXPAY_MYR_New"></include>
                  	<!--method not available as of June 10 <include name="testBridgePayNew"></include>-->
               </methods>
           </class>
       </classes>
   </test>
   <test name="UM Vietnam Deposit">
       <parameter name="TraderName" value="testcrm_donttouch_cpsvietnamclient@test.com"></parameter>
       <classes>
           <class name="newcrm.testcases.cptestcases.DepositTestCases">
               <methods>
                   <include name="cpsVietnamBTZotaPayNew"></include>
                   <include name="testVietnamBTEeziePay_New"></include>
                   <include name="testWowsPay_Remit_VND_New"></include>
               </methods>
           </class>
       </classes>
   </test>
	<test name="UM Brazil PIX Deposit">
		<parameter name="TraderName" value="testcrm_donttouch_cpsbrazil@test.com"></parameter>
		<classes>
			<class name="newcrm.testcases.cptestcases.DepositTestCases">
				<methods>
					<!--<include name="cpsBrazilBT_Bitolo_PixNew"></include>-->
					<include name="cpsBrazilBT_Toppay_PixNew"></include>
				</methods>
			</class>
