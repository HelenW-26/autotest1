<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UM APP Test Suite" configfailurepolicy="continue">

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<listeners>
     	<listener class-name="utils.Listeners.CustomListener"/>
  	</listeners>
  	<parameter name="Brand" value="UM"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
  	<parameter name="Regulator" value="svg"></parameter>
  	<parameter name="APPURL" value="${TraderURL}"></parameter>
  		
  		
  	<test name="UM Test APP CPS Taiwan Bank Transfer Withdrawal - batch 5">
   	<parameter name="UserId" value="832714"></parameter>
   	<parameter name="AccountNo" value="899188"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsTWBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test> 
  	<test name="UM Test APP CPS Brazil BT / Brazil PIX Withdrawal - batch 5.5">
   	<parameter name="UserId" value="832761"></parameter>
   	<parameter name="AccountNo" value="899227"></parameter>
    <classes>
   		 <class name="newcrm.testcases.app.APPCPSWithdrawalRegression">
   		 	<methods>
   		 		<include name="cpsBRLBTWithdraw"></include>
   		 		<include name="cpsPIXBTWithdraw"></include>
   		 	</methods>
   		 </class>   		      		
    </classes>
  	</test> 
  
  <!-- Test -->
</suite> <!-- Suite -->
