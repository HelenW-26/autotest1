    <parameter name="TraderURL" value="${TraderURL}"></parameter>
    <parameter name="TraderPass" value="123Qwe@@"></parameter>
    <parameter name="headless" value="true"></parameter>


    <test name="STAR IB E-Wallet Withdraw">
        <parameter name="Brand" value="STAR"></parameter>
        <parameter name="TraderName" value="testcrm_donttouch_cpschinaclient@test.com"></parameter>
        <classes>
            <class name="newcrm.testcases.ibtestcases.IBRebateWithdrawTestcases">
                <methods>
                    <include name="testPerfectMoneyWithdraw"></include>
                    <include name="testSticPayWithdraw"></include>
                </methods>
            </class>
        </classes>
    </test>
