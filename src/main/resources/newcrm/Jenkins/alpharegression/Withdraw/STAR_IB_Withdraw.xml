<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="STAR IB Test Suite" configfailurepolicy="continue">
    <listeners>
        <listener class-name="utils.Listeners.CustomListener"/>
