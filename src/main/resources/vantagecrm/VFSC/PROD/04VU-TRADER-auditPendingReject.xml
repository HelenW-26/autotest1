<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite allow-return-values="false" configfailurepolicy="continue" data-provider-thread-count="10" group-by-instances="false" guice-stage="DEVELOPMENT" junit="false" name="KY_Regression_Suite" parallel="none" preserve-order="true" skipfailedinvocationcounts="false" thread-count="5">

 <listeners>
      <listener class-name="utils.Listeners.CustomListener"/>
  </listeners>

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<parameter name="Brand" value="vfsc"/>
  	<parameter name="TestEnv" value="prod"/>
    <parameter name="AdminURL" value="https://admin.vantagefx.com"/>
  	<parameter name="AdminName" value="Test Crm"/>
  	<parameter name="AdminPass" value="Vantage123"/>
  	<parameter name="TraderURL" value="https://secure.vantagefx.com"/>
   	 <parameter name="TraderName" value="webopn@test.com"/>
    <parameter name="TraderPass" value="123Qwe"/>  
	
  <test allow-return-values="false" group-by-instances="false" junit="false" name="KY_Deposit_Audit" preserve-order="true" skipfailedinvocationcounts="false">
    <classes>
 	 <class name="vantagecrm.TraderDeposit">  
 		<methods>
 			<include name="TraderLogIn"/>
 			<include name="CCDeposit"/> 		 	
 			<include name="I18NDeposit"/>      
 			<include name="BrokerDeposit"/>
 			<include name="UnionPayDeposit"/> 
 			<include name="MobilepayDeposit"/>
			<include name="UnionPayP2PDeposit"/>
		</methods> 	
	  </class>
	  
	  <class name="vantagecrm.TaskManagement">
      
      		<methods>
	      		<include name="AdminLogIn"/>
	       		<include name="OfflineDepositPending"/>
				<include name="OfflineDepositPendingAll"/>
       		</methods>
      </class>
    </classes>  
	</test>
	
	<test allow-return-values="false" group-by-instances="false" junit="false" name="KY_Withdraw_Audit" preserve-order="true" skipfailedinvocationcounts="false">
		<classes>
		
			 <class name="vantagecrm.TraderWithdraw">
      			<methods>
		            <include name="TraderLogIn"/> 
		           	<include name="WithdrawCC"/>
		      		<include name="WithdrawI18NBank"/>  
					<include name="WithdrawAUBank"/>
					<include name="WithdrawChinaBank"/>
					<include name="WithdrawSkrNet"/>
					<include name="WithdrawFasaPay"/>
					<include name="WithdrawUnionPay"/>
       			</methods>
       		</class>   
		
			       
		<class name="vantagecrm.TaskManagement">
	      
	      		<methods>
		      		<include name="AdminLogIn"/>
		      		<include name="WithdrawlPending"/>
		      		<include name="WithdrawlPendingAll"/>
		      		<include name="WithdrawlAuditReject"/>  
	       		</methods>
	      </class>
		</classes>
	</test>
  
  <test allow-return-values="false" group-by-instances="false" junit="false" name="Trader_MyAccount_changeLeverage1" preserve-order="true" skipfailedinvocationcounts="false">
  	<classes>
  	     <class name="vantagecrm.TraderMyAccount">
       		<methods>
       			<include name="TraderLogIn"/>
      			<include name="changeLeverage"/>
       		</methods>
       </class>
  
      <class name="vantagecrm.TaskManagement">
      
      		<methods>
	      		<include name="AdminLogIn"/>
	      		<include name="leveragePending"/>  
		     
       		</methods>
      </class>
  	
  	
  	</classes>
  
  </test>
  
  <test allow-return-values="false" group-by-instances="false" junit="false" name="Trader_MyAccount_changeLeverage2" preserve-order="true" skipfailedinvocationcounts="false">
  	<classes>
  	     <class name="vantagecrm.TraderMyAccount">
       		<methods>
       			<include name="TraderLogIn"/>
      			<include name="changeLeverage"/>
       		</methods>
       </class>
  
      <class name="vantagecrm.TaskManagement">
      
      		<methods>
	      		<include name="AdminLogIn"/>
	      		<include name="leverageReject"/>  
		     
       		</methods>
      </class>
  	
  	
  	</classes>
  
  </test>
  
</suite><!-- Suite -->