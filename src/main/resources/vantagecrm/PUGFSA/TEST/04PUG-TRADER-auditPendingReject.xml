       			<include name="TraderLogIn"></include>
      			<include name="changeLeverage"></include>
       		</methods>
       </class>
  
      <class name="vantagecrm.TaskManagement">
      
      		<methods>
	      		<include name="AdminLogIn"></include>
	      		<include name="leveragePending"></include>  
		     
       		</methods>
      </class>
  	
  	
  	</classes>
  
  </test>
  
  <test name="Trader_MyAccount_changeLeverage2">
