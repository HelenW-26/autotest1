<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Create New Users" configfailurepolicy="continue">

 <listeners>
     <listener class-name="utils.Listeners.CustomListener"/>
     <listener class-name="utils.Listeners.RetryListener"/>
  </listeners>

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	
	<parameter name="headless" value="true"/>
	  	
  	<parameter name="AdminName" value="Test CRM"></parameter>
  	<parameter name="AdminPass" value="VanSyd123"></parameter>
  	
	<!-- AU -->	
  
  <test name="Create_AU_VFSC2_User">
    <parameter name="Brand" value="vfsc2"/>
  	<parameter name="TestEnv" value="prod"/>
  	<parameter name="AdminURL" value="https://admin.vantagefx.com"></parameter>
    <classes>

     <class name="vantagecrm.CPRegister">
        <parameter name="WebSiteURL" value="https:/www.vantagefx.com/"/>
      	<parameter name="RegisterURL" value=""/>
        <parameter name="IBpromotion" value="?affid=NBMjExOTQz"/>    
     
     	<methods>
     		<include name ="RegisterUserMT4"></include>
       	</methods>    
     </class>
      <class name="vantagecrm.AdminAPITest">
	   	  	<methods>
	   	  		<include name="APIUpdateTestAccGroup"/>
	   	  	</methods>
	  </class> 
    </classes>
  </test>
  
