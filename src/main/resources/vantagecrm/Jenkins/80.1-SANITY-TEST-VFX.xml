<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Create New User and Audit" configfailurepolicy="continue">

 <listeners>
     <listener class-name="utils.Listeners.CustomListener"></listener>
     <listener class-name="utils.Listeners.RetryListener"/>
  </listeners>

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod".
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->


  <test name="Create_NewUser_and_Audit">
    <parameter name="Brand" value="vfsc2"></parameter>
  	<parameter name="TestEnv" value="alpha"></parameter>
    <parameter name="AdminURL" value="https://staging-au-admin.test-vantagefx.com"></parameter>
  	<parameter name="TraderURL" value="https://staging-au-secure.test-vantagefx.com"></parameter>
  	<parameter name="CallbackUrl" value="https://staging-au-openapi.test-vantagefx.com"></parameter>
  	
     <parameter name="TraderName" value="testcrmibtgc@auto-test.com"></parameter>
     <parameter name="TraderPass" value="123Qwe"></parameter>
  	<parameter name="AdminName" value="automation_au"></parameter>
  	<parameter name="AdminPass" value="123Qwe"></parameter>
  	
  	
  	<parameter name="headless" value="false"/>
    

    
    <groups>
    	<run>
    		<include name="sanity"></include>
    	</run>
    </groups>
    
    <classes>

	    <class name="vantagecrm.CPRegister">
	        <parameter name="WebSiteURL" value="https://new.vantagefx.com/forex-trading/forex-trading-account"></parameter>
	      	<parameter name="RegisterURL" value="https://staging-au-secure.test-vantagefx.com"></parameter>
	      	<parameter name="IBpromotion" value=""></parameter>
	     </class>
	     
 		 <class name="vantagecrm.CPDeposit"> </class> 
 		 <class name="vantagecrm.CPNewWithdraw"> </class>
		 <class name="vantagecrm.TaskManagement"> </class>
		 

    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
