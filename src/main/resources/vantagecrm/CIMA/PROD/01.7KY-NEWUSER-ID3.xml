<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Create New User and Audit" configfailurepolicy="continue">

 <listeners>
     <listener class-name="utils.Listeners.CustomListener"></listener>
  </listeners>

	<!-- this parameter is used for Account Audit option. Value can be "test", "beta" or "prod". 
	When value is set to "beta" or "prod", the field Accout_Group_Setting can only be one test settings, like test_crm1_USD -->
	<parameter name="testEnv" value="test"></parameter>
	
  <test name="Verify ID3 With PROD Info">
    <parameter name="Brand" value="ky"></parameter>
  	<parameter name="TestEnv" value="prod"></parameter>
   	<parameter name="WebSiteURL" value="http://www.vantagefx.com"></parameter>
   	<parameter name="RegisterURL" value=""></parameter>
     <parameter name="IBpromotion" value="?affid=NBMjExOTQz"></parameter>    
    <classes>

     <class name="vantagecrm.CPID3WithProdUser">

     
     	<methods>
       		<include name ="RegisterUserMT4"></include>
 		</methods>    
 
     </class>        
  
 
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
