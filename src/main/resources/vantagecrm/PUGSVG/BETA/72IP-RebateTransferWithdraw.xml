<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Client Portal Test Suite PUG"
	configfailurepolicy="continue">

	<listeners>
		<listener class-name="utils.Listeners.CustomListener"></listener>
	</listeners>

	<parameter name="Brand" value="svg"></parameter>
	<parameter name="TestEnv" value="beta"></parameter>
	<parameter name="AdminURL" value="https://betavgpadmin.puprime.com"></parameter>
	<parameter name="AdminName" value="Test CRM"></parameter>
	<parameter name="AdminPass" value="Vantage123"></parameter>
	<parameter name="TraderURL" value="https://betavgpmyaccount.puprime.com"></parameter>
	<parameter name="TraderName" value="ibtestolr@test.com"></parameter>
	<parameter name="TraderPass" value="123Qwe"></parameter>


	<test name="IB Portal Rebate Transfer from Dashboard">
		<classes>

			<class name="vantagecrm.UpdateRebateACC">
				<methods>
					<include name="AdminLogIn"></include>
					<include name="adjustDeposit"></include>
				</methods>
			</class>

			<class name="vantagecrm.IPRebate">
				<methods>
					<include name="IPLogIn"></include>
					<include name="TransferRebateFromDashboard"></include>
				</methods>
			</class>

		</classes>
	</test>

	<test name="IB Portal Rebate Transfer from PaymentHistory">
		<classes>

			<class name="vantagecrm.UpdateRebateACC">
				<methods>
					<include name="AdminLogIn"></include>
					<include name="adjustDeposit"></include>
				</methods>
			</class>

			<class name="vantagecrm.IPRebate">
				<methods>
					<include name="IPLogIn"></include>
					<include name="TransferRebateFromPaymentHistory"></include>
				</methods>
			</class>

		</classes>
	</test>

	<test name="IB Portal Withdraw">
		<classes>

			<class name="vantagecrm.UpdateRebateACC">
				<methods>
					<include name="AdminLogIn"></include>
					<include name="adjustDeposit"></include>
				</methods>
			</class>

			<class name="vantagecrm.IPWithdraw">
				<methods>
					<include name="IPLogIn"></include>
					<include name="WithdrawI18NBank"></include>
					<include name="WithdrawUnionPayCPS"></include>
				</methods>
			</class>

			<class name="vantagecrm.TaskManagement">
				<methods>
					<include name="AdminLogIn"></include>
					<include name="WithdrawlAudit"></include>
					<include name="WithdrawlAuditComplete"></include> 
				</methods>
			</class>

		</classes>
	</test>

</suite> <!-- Suite -->
